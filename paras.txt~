    // Sign up for updates form
    $('#uSubscribe').click(function(){
      $('#footerformloading').show();
      $('#uSubscribe').prop('disabled',true);
      // $.post('/subscribeupdates.php', {'email':$('#uEmail').val()},function(data){
        $.post('/subscribeupdates.php', {'email':$('#uEmail').val(),'gdpr':$("input[name='gdpr_check']:checked").val()},function(data){
        $('#footerformloading').hide();
        $('#uSubscribe').prop('disabled',false);
        if (data.error != '') {
          $('#uEmailerror').html('<strong>'+data.error+'</strong>').show();
        } else {

          // Altitude downloads click tracking
          if (typeof ire === 'function') {
           var sku='Sign Up For Updates';
           var orderId='ad9642f1ec4b9c0f377bb866b14a35eefooter';
        
           ire('trackConversion', 16627, {
            orderId: orderId,
            customerId: data.shaemail,
            customerEmail: data.shaemail,
            items: [{
             subTotal: 0.00,  //always 0.00
             category: 'footer',  //type of download
             sku: sku,  //or title of item downloaded if no sku available
             quantity: 1  //always 1
            }]
           }, {verifySiteDefinitionMatch:true});
          }

          $('#footerformarea').hide(function(){
            $('#footerformthanks').show().delay(4000).fadeOut('slow',function(){
              $('#uEmail').val('');
              $('#footerformarea').fadeIn('slow');
            });
          });
        }
      });
    });

    $('#uEmail').focus(function(){
      $('#uEmailerror').hide();
    });
    // /Sign up for updates form

    $("#att_map").click(function(e){
      // $('#att_panel_map').show();
      $('#att_panel_map').addClass('active');
      $('#att_panel_list').hide();
      $('#att_panel_grid').hide();

      $('#att_order_toggle').hide();
      $('#att_filter_mask_order').hide();
      $('#att_order_toggle').removeClass("active");

      var center = map.getCenter();
      google.maps.event.trigger(map, "resize");
      map.setCenter(center);

      $("#right_att_map").fadeOut();

      $(".right_att_toggle a").removeClass('active');
      $(".right_att_toggle_map").addClass('active');

      e.preventDefault();

      createCookie('att_view', 'map', 365);

    });

    $(".right_att_toggle_map").click(function(e){
        // $('#att_panel_map').show();
        $('#att_panel_map').addClass('active');
        $('#att_panel_list').hide();
        $('#att_panel_grid').hide();

        $('#att_order_toggle').hide();
        $('#att_filter_mask_order').hide();
        $('#att_order_toggle').removeClass("active");

        $("#right_att_map").fadeOut();

        $(".right_att_toggle a").removeClass('active');
        $(this).addClass('active');
      
        e.preventDefault();

        createCookie('att_view', 'map', 365);
    });

    $(".right_att_toggle_list").click(function(e){
        // $('#att_panel_map').hide();
        $('#att_panel_map').removeClass('active');
        $('#att_panel_list').show();
        $('#att_panel_grid').hide();

        $('#att_order_toggle').show();

        $("#right_att_map").fadeIn();

        $(".right_att_toggle a").removeClass('active');
        $(this).addClass('active');

        e.preventDefault();

        createCookie('att_view', 'list', 365);
    });

    $(".right_att_toggle_grid").click(function(e){
        // $('#att_panel_map').hide();
        $('#att_panel_map').removeClass('active');
        $('#att_panel_list').hide();
        $('#att_panel_grid').show();

        $('#att_order_toggle').show();

        $("#right_att_map").fadeIn();

        $(".right_att_toggle a").removeClass('active');
        $(this).addClass('active');

        e.preventDefault();

        createCookie('att_view', 'grid', 365);
    });

