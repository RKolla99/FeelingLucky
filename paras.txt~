    $("#itinerary_menu ol a").click(function(e){

        $("#itinerary_menu ol a").removeClass('active');
        $(this).addClass('active');

        var $anchor = $(this);
        $(".itinerary_scroller_day").removeClass('itinerary_scroller_day_active');
        $($anchor.attr('href')).addClass('itinerary_scroller_day_active');

        if ($anchor.attr('href') == '#itinerary_scroller_day_3') {
        	$("#itinerary_menu h2 span").text("3");
        } else if ($anchor.attr('href') == '#itinerary_scroller_day_5') {
        	$("#itinerary_menu h2 span").text("5");
        } else {
        	$("#itinerary_menu h2 span").text("7");
        }

        $('html, body').stop().animate({scrollTop: $($anchor.attr('href')).offset().top }, 'slow');

        e.preventDefault();
    }); 


