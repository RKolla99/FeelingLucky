    $("#att_filter_toggle").click(function(e){
        if($('#att_filter_mask_filters').is(':visible')){
        } else {
          $('.att_filter_mask').hide();
          $('#att_filter_top a').removeClass("active");
        }
        // $('#att_filter_mask_filters').slideToggle("slow");
        $('#att_filter_mask_filters').toggle();
        $(this).toggleClass("active");
        e.preventDefault();
    });

    $("#att_order_toggle").click(function(e){
        if($('#att_filter_mask_order').is(':visible')){
        } else {
          $('.att_filter_mask').hide();
          $('#att_filter_top a').removeClass("active");
        }
        // $('#att_filter_mask_order').slideToggle("slow");
        $('#att_filter_mask_order').toggle();
        $(this).toggleClass("active");
        e.preventDefault();
    });

    setTimeout(function() {
      // $("#att_review").addClass("in_view");
      $("#att_review_fixed").addClass("in_view");   
    }, 2000);

    setTimeout(function() {
      $("#att_review_fixed").removeClass("in_view");    
    }, 12000);

    $(".att_review_fixed_close").click(function(e){
      $("#att_review_fixed").removeClass("in_view");
      e.preventDefault();
    }); 
    
    // language menu    

    $('#header_main .menu_language .open').click(function(e) {
      e.preventDefault();
      $(this).closest('#header_main .menu_language').find('ul').animate( {opacity: 'toggle', height: 'toggle'}, 50 );
    });

    $("#header_main #menu_languages").mouseleave(function(){
        $("#header_main #menu_languages").delay(1000).fadeOut(100);
    });

    $(".open-panel").click(function(){
      // $(this).next().slideToggle("slow");
      $(this).next().animate( {opacity: 'toggle', height: 'toggle'} );
      $(this).toggleClass("opened");
    });

